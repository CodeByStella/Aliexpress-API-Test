# AliExpress Open Platform (openservice.aliexpress.com)
# Get app_key and app_secret from your app in the console.

app_key=your_app_key
app_secret=your_app_secret

# Redirect URI configured in your app settings (for OAuth callback)
REDIRECT_URI=https://your-callback-url.com/api/callback

# One-time auth code from user authorization (only for get-access-token)
# After getting tokens, you use access_token and refresh_token instead.
AUTH_CODE=

# After running get-access-token, copy the returned values here:
access_token=
refresh_token=
